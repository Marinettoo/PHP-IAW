<?php
/*Write a script that points out the number of times that a string is 
contained inside another string*/

$str = "Hola soy Jesús, Jesús Pérez Marinetto,";
$longitud_str = strlen($str);
$substr = "Jesús";

for ($i = 0; $i <= $longitud_str;$i++){
    $a_buscar = count($substr);
}

echo ($a_buscar)

?>